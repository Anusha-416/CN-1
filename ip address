#include<stdio.h>
int main()
{
int i,ip;
char subs[25],str[25];
printf("enter your ip address");
scanf("%s",str)
for(i=0;i<=2;i++)
{
subs[i]=str[i];
}
printf(subs)
ip=atos(subs)
if((ip>0)&&(ip<128))
printf("given ip address is %s belongs to class A:",str);
if((ip>127)&&(ip<192))
printf("given ip address is belongs to class B:",str);
if((ip>191)&&(ip<224))
printf("given ip address is %s belongs to class C:",str);
if((ip>223)&&(ip<240))
printf("given ip address is %s belongs to class D:",str);
if(ip>223)
printf("given ip address is %s belongs to class E:",str);
}


